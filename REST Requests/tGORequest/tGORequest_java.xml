<?xml version="1.0" encoding="UTF-8"?>
<COMPONENT> 

<HEADER   AUTHOR="Francois.Belleau@bio2rdf.org" 
          COMPATIBILITY="ALL" 
          PLATEFORM="ALL" 
          RELEASE_DATE="20140119" 
          SERIAL="" 
          STARTABLE="true" 
          STATUS="BETA" 
          DATA_AUTO_PROPAGATE="false"
          SCHEMA_AUTO_PROPAGATE="false"
          HAS_CONDITIONAL_OUTPUTS="true"
          PARTITIONING="AUTO"
          VERSION="0.1">    
          <SIGNATURE/>
</HEADER>

<FAMILIES> 
  <FAMILY>Bio2RDF</FAMILY>
</FAMILIES>  

<DOCUMENTATION> 
  <URL>http://bio2rdf.org</URL> 
</DOCUMENTATION> 

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER NAME="PROPERTY" FIELD="SCHEMA_TYPE" READONLY="true" REQUIRED="true" NUM_ROW="1">
      <TABLE READONLY="true">
        <COLUMN NAME="RDF" TYPE="id_String" READONLY="true" CUSTOM="true" />
      </TABLE>
    </PARAMETER>
    
    <PARAMETER NAME="REST_URI" FIELD="TEXT" REQUIRED="true" NUM_ROW="2">
      <DEFAULT>"http://rest.bio2rdf.org"</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="ENDPOINT_MODE" FIELD="CHECK" NUM_ROW="3" SHOW_IF="'false'">
      <DEFAULT>true</DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="ENDPOINT_STRING" FIELD="STRING" NUM_ROW="4" REQUIRED="true" SHOW_IF="'false'">
      <DEFAULT>"go"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="ENDPOINT_CHOSEN" FIELD="CLOSED_LIST" NUM_ROW="5" REQUIRED="false" SHOW_IF="'true'">
      <ITEMS DEFAULT="go">
        <ITEM NAME="go" VALUE="go"/>
      </ITEMS>
    </PARAMETER>

    <PARAMETER NAME="METHOD_STRING" FIELD="STRING" NUM_ROW="6" REQUIRED="true" SHOW_IF="'false'">
      <DEFAULT>"describe"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="METHOD_CHOSEN" FIELD="CLOSED_LIST" NUM_ROW="10">
      <ITEMS DEFAULT="DESCRIBE">
        <ITEM NAME="DESCRIBE" VALUE="describe"/>
        <ITEM NAME="DESCRIBE_URI" VALUE="describe_uri"/>
        <ITEM NAME="LINKS" VALUE="links"/>
        <ITEM NAME="LINKS_URI" VALUE="links_uri"/>
        <ITEM NAME="SEARCH" VALUE="search"/>
        <ITEM NAME="LIST" VALUE="list"/>
      </ITEMS>
    </PARAMETER>

    <PARAMETER NAME="RDF_URI" FIELD="TEXT" REQUIRED="true" SHOW_IF="(METHOD_CHOSEN == 'DESCRIBE_URI') OR (METHOD_CHOSEN == 'LINKS_URI')" NUM_ROW="12">
      <DEFAULT>"http://www.geneontology.org/go#GO:0004673"</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="BIO2RDF_URI" FIELD="TEXT" REQUIRED="true" SHOW_IF="(METHOD_CHOSEN == 'DESCRIBE') OR (METHOD_CHOSEN == 'LINKS')" NUM_ROW="13">
      <DEFAULT>"go:0004673"</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="QUERY" FIELD="TEXT" REQUIRED="true" SHOW_IF="METHOD_CHOSEN == 'SEARCH'"  NUM_ROW="14">
      <DEFAULT>"hexokinase"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="FORMAT_CHOSEN" FIELD="CLOSED_LIST" NUM_ROW="15">
      <ITEMS DEFAULT="NT">
        <ITEM NAME="NT" VALUE="nt"/>
        <ITEM NAME="RDF" VALUE="rdf"/>
        <ITEM NAME="N3" VALUE="n3"/>
        <ITEM NAME="JSON" VALUE="json"/>
        <ITEM NAME="JSONLD" VALUE="jsonld"/>
      </ITEMS>
    </PARAMETER>
    
    <PARAMETER NAME="POST_PARAMS_FROM_FILE" FIELD="FILE" SHOW_IF="'false'" NUM_ROW="20">
      <DEFAULT/>
    </PARAMETER>

    <PARAMETER NAME="OUT_TO_FILE" FIELD="CHECK" REQUIRED="true" SHOW_IF="'false'"  NUM_ROW="21">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="OUT_FILE_NAME" FIELD="FILE" SHOW_IF="OUT_TO_FILE == 'true'" NUM_ROW="22" >
      <DEFAULT/>
    </PARAMETER>
    
    <PARAMETER NAME="HEADERS" FIELD="TABLE" REQUIRED="false" NB_LINES="7" NUM_ROW="35" SHOW_IF="'false'">
      <ITEMS>
        <ITEM NAME="HEADER_NAME" />
        <ITEM NAME="HEADER_VALUE" />
      </ITEMS>
    </PARAMETER>
    
    <PARAMETER NAME="DEBUG_MODE" FIELD="CHECK" NUM_ROW="50">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="NEED_AUTHENTICATION" FIELD="CHECK" NUM_ROW="40" SHOW_IF="'false'">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="USR" FIELD="TEXT" NUM_ROW="40" SHOW_IF="NEED_AUTHENTICATION == 'true'">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="PWD" FIELD="TEXT" NUM_ROW="40" SHOW_IF="NEED_AUTHENTICATION == 'true'">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
  </PARAMETERS>

  <CODEGENERATION/>

  <RETURNS/>

</COMPONENT>