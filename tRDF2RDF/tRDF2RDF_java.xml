<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    STARTABLE="false"
    COMPATIBILITY="ALL"
    AUTHOR="Bio2RDF"
    RELEASE_DATE="20130919"
    DATA_AUTO_PROPAGATE="false"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Semantic Web</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
	<PARAMETER NAME="OUTPUT_SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="1">
	</PARAMETER>
	
	<PARAMETER
		NAME="INPUTFORMAT"
		FIELD="CLOSED_LIST"
		NUM_ROW="5"
		REQUIRED="true"
		>
		<ITEMS DEFAULT="XML">
			<ITEM NAME="XML" VALUE="RDF/XML" />
			<ITEM NAME="TRIPLE" VALUE="N-TRIPLE" />
			<ITEM NAME="TURTLE" VALUE="TURTLE" />
		</ITEMS>
	</PARAMETER>  
	
	<PARAMETER
		NAME="OUTPUTFORMAT"
		FIELD="CLOSED_LIST"
		NUM_ROW="10"
		REQUIRED="true"
		>
		<ITEMS DEFAULT="XML">
			<ITEM NAME="XML2" VALUE="RDF/XML" />
			<ITEM NAME="TRIPLE2" VALUE="N-TRIPLE" />
			<ITEM NAME="TURTLE2" VALUE="TURTLE" />
		</ITEMS>
	</PARAMETER> 
	
   </PARAMETERS>

   <ADVANCED_PARAMETERS>
   <PARAMETER NAME="IMPORT" FIELD="MEMO_IMPORT" REQUIRED="false" SHOW_IF="false" NUM_ROW="0" NB_LINES="3">
	    <DEFAULT>import com.hp.hpl.jena.rdf.model.*; import com.hp.hpl.jena.util.FileManager; import java.io.*; import org.openjena.riot.out.RDFJSONWriter;</DEFAULT>
	  </PARAMETER>

   </ADVANCED_PARAMETERS>

   <CODEGENERATION>
	   <IMPORTS>
	      <IMPORT NAME="jena-core-2.7.2" MODULE="jena-core-2.7.2.jar" REQUIRED="true" />
		  <IMPORT NAME="slf4j-log4j12-1.6.4" MODULE="slf4j-log4j12-1.6.4.jar" REQUIRED="true" />
		  <IMPORT NAME="slf4j-api-1.6.4" MODULE="slf4j-api-1.6.4.jar" REQUIRED="true" />
		  <IMPORT NAME="log4j-1.2.16" MODULE="log4j-1.2.16.jar" REQUIRED="true" />
		  <IMPORT NAME="xercesImpl-2.10.0" MODULE="xercesImpl-2.10.0.jar" REQUIRED="true" />
		  <IMPORT NAME="xml-apis-1.4.01" MODULE="xml-apis-1.4.01.jar" REQUIRED="true" />
		  <IMPORT NAME="jena-iri-0.9.2" MODULE="jena-iri-0.9.2.jar" REQUIRED="true" />
		  <IMPORT NAME="jena-arq-2.9.2" MODULE="jena-arq-2.9.2.jar" REQUIRED="true" />
	   </IMPORTS>
   </CODEGENERATION>

   <RETURNS>
      <RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER"/>
   </RETURNS>
</COMPONENT>