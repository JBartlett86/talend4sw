<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20070806A"
    STARTABLE="true"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Talend4SW/Virtuoso</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
	
    <PARAMETER
      NAME="URL"
      FIELD="TEXT"
      NUM_ROW="15"
      REPOSITORY_VALUE="URL"
      REQUIRED="true">
      <DEFAULT>"jdbc:virtuoso://localhost:1111/DATABASE=DB/UID=dba/PWD=dba"</DEFAULT>
    </PARAMETER>
	
    <PARAMETER
      NAME="DRIVER_CLASS"
      FIELD="TEXT"
      NUM_ROW="25"
      REPOSITORY_VALUE="DRIVER_CLASS"
      REQUIRED="true">
      <DEFAULT>"virtuoso.jdbc3.Driver"</DEFAULT>
    </PARAMETER>
	
    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="30"
      REPOSITORY_VALUE="USERNAME"
      REQUIRED="true">
      <DEFAULT>"dba"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="PASS"
      FIELD="TEXT"
      NUM_ROW="30"
      REPOSITORY_VALUE="PASSWORD"
      REQUIRED="true">
      <DEFAULT>"dba"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      NUM_ROW="35"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

 	<PARAMETER
      NAME="GUESS_SCHEMA"
      FIELD="GUESS_SCHEMA"
      NUM_ROW="45"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
	
    <PARAMETER
      NAME="QUERY"
      FIELD="MEMO_SQL"
      NUM_ROW="55"
      REQUIRED="true"
    >
      <DEFAULT>"select count(*) where {?s ?p ?o .}"</DEFAULT>
    </PARAMETER>
		
   </PARAMETERS>

   <ADVANCED_PARAMETERS>
	<PARAMETER
      	NAME="ENCODING"
      	FIELD="ENCODING_TYPE"
      	NUM_ROW="10"
      	REQUIRED="true"
      	SHOW="false"
    >
      	<DEFAULT>"ISO-8859-15"</DEFAULT>
	</PARAMETER>
	
	<PARAMETER NAME="USE_CURSOR" FIELD="CHECK" NUM_ROW="20">
		<DEFAULT>false</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="CURSOR_SIZE" FIELD="TEXT" NUM_ROW="20"
			SHOW_IF="(USE_CURSOR == 'true')">
			<DEFAULT>1000</DEFAULT>
	</PARAMETER>

	
	
	<PARAMETER NAME="TRIM_ALL_COLUMN" FIELD="CHECK" NUM_ROW="30">
		<DEFAULT>false</DEFAULT>
	</PARAMETER>
	
	<PARAMETER NAME="TRIM_COLUMN" FIELD="TABLE" NUM_ROW="40"
		NB_LINES="5" SHOW_IF="TRIM_ALL_COLUMN == 'false'">
		<ITEMS BASED_ON_SCHEMA="true">
			<ITEM NAME="TRIM" FIELD="CHECK"></ITEM>
		</ITEMS>
	</PARAMETER>		
   </ADVANCED_PARAMETERS>

   <CODEGENERATION>
   <IMPORTS>
   	<IMPORT NAME="virtuoso.jdbc3.Driver" MODULE="virtjdbc.jar" REQUIRED="true"></IMPORT></IMPORTS></CODEGENERATION>

   <RETURNS>
      <RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER"/>
      <RETURN NAME="QUERY" TYPE="id_String" AVAILABILITY="FLOW"/>
   </RETURNS>
</COMPONENT>