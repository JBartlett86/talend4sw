<?xml version="1.0" encoding="UTF-8"?>
<COMPONENT> 

<HEADER AUTHOR="Bio2RDF" 
          COMPATIBILITY="ALL" 
          PLATEFORM="ALL" 
          RELEASE_DATE="20140827" 
          SERIAL="" 
          STARTABLE="true" 
          STATUS="BETA" 
          DATA_AUTO_PROPAGATE="false"
          SCHEMA_AUTO_PROPAGATE="false"
          PARTITIONING="AUTO"
          VERSION="0.1">
          
<SIGNATURE/> 
</HEADER>

  <FAMILIES> 
    <FAMILY>Semantic Web/Sesame</FAMILY>
  </FAMILIES>  
  <DOCUMENTATION> 
    <URL></URL> 
  </DOCUMENTATION>   
  <CONNECTORS> 
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK" />
    <CONNECTOR CTYPE="COMPONENT_ERROR" />
    <CONNECTOR CTYPE="RUN_IF" />
  </CONNECTORS>  
  
  <PARAMETERS>
 
 <PARAMETER
      NAME="SESAMEMODEL"
      FIELD="COMPONENT_LIST"
      FILTER="tSesameModelCreate"
      NUM_ROW="3"
      DYNAMIC_SETTINGS="true"
    />
  
<PARAMETER NAME="OUTPUT_SCHEMA" FIELD="SCHEMA_TYPE" READONLY="false" REQUIRED="true" NUM_ROW="1">
<TABLE READONLY="true">
<COLUMN NAME="subject" TYPE="id_String" READONLY="false" CUSTOM="true" />
<COLUMN NAME="predicate" TYPE="id_String" READONLY="false" CUSTOM="true" />
<COLUMN NAME="object" TYPE="id_String" READONLY="false" CUSTOM="true" />
<COLUMN NAME="graph" TYPE="id_String" READONLY="false" CUSTOM="true" />
<COLUMN NAME="type" TYPE="id_String" READONLY="false" CUSTOM="true" />
</TABLE>
</PARAMETER>
	
	<PARAMETER NAME="CHECK_SUBJECT" FIELD="CHECK" NUM_ROW="30">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
	
	<PARAMETER NAME="FILTER_SUBJECT" FIELD="TEXT" REQUIRED="false" SHOW_IF="CHECK_SUBJECT == 'true'" NUM_ROW="30">
      <DEFAULT>null</DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="CHECK_PREDICATE" FIELD="CHECK" NUM_ROW="31">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
	
	<PARAMETER NAME="FILTER_PREDICATE" FIELD="TEXT" REQUIRED="false" SHOW_IF="CHECK_PREDICATE == 'true'" NUM_ROW="31">
      <DEFAULT>null</DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="CHECK_OBJECT" FIELD="CHECK" NUM_ROW="32">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="FILTER_OBJECT" FIELD="TEXT" REQUIRED="false" SHOW_IF="CHECK_OBJECT == 'true'" NUM_ROW="32">
      <DEFAULT>null</DEFAULT>
    </PARAMETER>
    
   	<PARAMETER NAME="CHECK_TYPE" FIELD="CHECK" SHOW_IF="CHECK_OBJECT == 'true'" NUM_ROW="32">
      <DEFAULT>true</DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="CHECK_GRAPH" FIELD="CHECK" NUM_ROW="33">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="FILTER_GRAPH" FIELD="TEXT" REQUIRED="false" SHOW_IF="CHECK_GRAPH == 'true'" NUM_ROW="33">
      <DEFAULT>null</DEFAULT>
    </PARAMETER>
   
  </PARAMETERS>
  
<ADVANCED_PARAMETERS>


</ADVANCED_PARAMETERS>

  <CODEGENERATION>
	   <IMPORTS>
	      <IMPORT NAME="openrdf-sesame-2.7.13-onejar.jar" MODULE="openrdf-sesame-2.7.13-onejar.jar" REQUIRED="true" />
		  <IMPORT NAME="slf4j-log4j12-1.7.7" MODULE="slf4j-log4j12-1.7.7.jar" REQUIRED="true" />
		  <IMPORT NAME="slf4j-api-1.7.7" MODULE="slf4j-api-1.7.7.jar" REQUIRED="true" />
		  <IMPORT NAME="log4j-1.2.17" MODULE="log4j-1.2.17.jar" REQUIRED="true" />
		  <IMPORT NAME="commons-io-2.4" MODULE="commons-io-2.4.jar" REQUIRED="true" />
	   </IMPORTS>
   </CODEGENERATION>
 
 
  <RETURNS> 
    <RETURN AVAILABILITY="AFTER" NAME="NB_LINE" TYPE="id_Integer"/> 
  </RETURNS> 
</COMPONENT>
