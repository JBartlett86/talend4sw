<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.101"
    STATUS="ALPHA"
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    DATA_AUTO_PROPAGATE="false"
    SCHEMA_AUTO_PROPAGATE="true"
    STARTABLE="true"
	PARTITIONING="AUTO"
    >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Semantic Web</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER NAME="PROPERTY" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="1">
      <TABLE READONLY="true">
        <COLUMN NAME="SparqlResults" TYPE="id_String" READONLY="true" CUSTOM="true" />
      </TABLE>
    </PARAMETER>

    
    <PARAMETER NAME="FORMAT_OUT" FIELD="CLOSED_LIST" NUM_ROW="15">
      <ITEMS DEFAULT="RDFXML">
        <ITEM NAME="RDFXML" VALUE="rdf"/>
        <ITEM NAME="TTL" VALUE="ttl"/>
        <ITEM NAME="NT" VALUE="nt"/>
      </ITEMS>
    </PARAMETER>
    
    <PARAMETER
      NAME="QUERY"
      FIELD="MEMO_SQL"
      NUM_ROW="55"
      REQUIRED="true"
    >
      <DEFAULT>"PREFIX up:&lt;http://purl.uniprot.org/core/&gt;
PREFIX taxon:&lt;http://purl.uniprot.org/taxonomy/&gt;
CONSTRUCT
{
	?protein a up:HumanProtein .
}
WHERE
{
	?protein a up:Protein .
	?protein up:organism taxon:9606
}
LIMIT 10"
      </DEFAULT>
    </PARAMETER>

  </PARAMETERS>
  
  <ADVANCED_PARAMETERS>
	   
	    <PARAMETER NAME="SPARQL_ENDPOINT" FIELD="TEXT" NUM_ROW="5">
	      <DEFAULT>"http://beta.sparql.uniprot.org/"</DEFAULT>
	    </PARAMETER>
   
	   <PARAMETER NAME="CHECK_DISPLAY_URL" FIELD="CHECK" NUM_ROW="10">
	      <DEFAULT>false</DEFAULT>
	   </PARAMETER>

   </ADVANCED_PARAMETERS>

  <CODEGENERATION/>

  <RETURNS/>

</COMPONENT>