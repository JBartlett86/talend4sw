<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.101"
    STATUS="ALPHA"
    COMPATIBILITY="ALL"
    AUTHOR="vincent.emonet@gmail.com"
    RELEASE_DATE="20050320A"
    DATA_AUTO_PROPAGATE="false"
    SCHEMA_AUTO_PROPAGATE="false"
    STARTABLE="true"
	PARTITIONING="AUTO"
    >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Bio2RDF</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER NAME="PROPERTY" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="1">
  </PARAMETER>
    
    <PARAMETER
      NAME="QUERY"
      FIELD="MEMO_SQL"
      NUM_ROW="55"
      REQUIRED="true"
    >
      <DEFAULT>"PREFIX sbmlrdf: &lt;http://identifiers.org/biomodels.vocabulary#&gt;

SELECT ?speciesid ?name 
WHERE {
  &lt;http://identifiers.org/biomodels.db/BIOMD0000000001&gt; sbmlrdf:species ?speciesid . 
  ?speciesid sbmlrdf:name ?name
}
LIMIT 10"</DEFAULT>
    </PARAMETER>

  </PARAMETERS>
  
  <ADVANCED_PARAMETERS>
  
	<PARAMETER NAME="SPARQL_ENDPOINT" FIELD="TEXT" NUM_ROW="5">
      <DEFAULT>"http://www.ebi.ac.uk/rdf/services/biomodels/sparql"</DEFAULT>
    </PARAMETER>
    
    	    <PARAMETER
      NAME="URL_PARAMETERS"
      FIELD="TABLE"
      NUM_ROW="8"
      NB_LINES="4"
    >
      <ITEMS>
        <ITEM NAME="KEY" FIELD="String" VALUE="&quot;key&quot;" />
        <ITEM NAME="VALUE" FIELD="String" VALUE="&quot;value&quot;" />
      </ITEMS>
    </PARAMETER>
   
	   <PARAMETER NAME="CHECK_DISPLAY_URL" FIELD="CHECK" NUM_ROW="10">
	      <DEFAULT>false</DEFAULT>
	   </PARAMETER>

   </ADVANCED_PARAMETERS>

  <CODEGENERATION>
		<IMPORTS>
			<IMPORT NAME="json-20140107.jar" MODULE="json-20140107.jar" REQUIRED="true" />
		</IMPORTS>
	</CODEGENERATION>

  <RETURNS/>

</COMPONENT>