<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.101"
    STATUS="ALPHA"
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    DATA_AUTO_PROPAGATE="false"
    SCHEMA_AUTO_PROPAGATE="true"
    STARTABLE="true"
	PARTITIONING="AUTO"
    >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Semantic Web</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER NAME="PROPERTY" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="1">
      <TABLE READONLY="true">
        <COLUMN NAME="SparqlResults" TYPE="id_String" READONLY="true" CUSTOM="true" />
      </TABLE>
    </PARAMETER>


	<PARAMETER NAME="TUMOR_TYPE" FIELD="CLOSED_LIST" NUM_ROW="10">
      <ITEMS DEFAULT="ACC">
        <ITEM NAME="ACC" VALUE="ACC"/>
        <ITEM NAME="BLCA" VALUE="BLCA"/>
        <ITEM NAME="BRCA" VALUE="BRCA"/>
        <ITEM NAME="DLBC" VALUE="DLBC"/>
        <ITEM NAME="ESCA" VALUE="ESCA"/>
        <ITEM NAME="CESC" VALUE="CESC"/>
        <ITEM NAME="HNSC" VALUE="HNSC"/>
        <ITEM NAME="KIRP" VALUE="KIRP"/>
        <ITEM NAME="LAML" VALUE="LAML"/>
        <ITEM NAME="LGG" VALUE="LGG"/>
        <ITEM NAME="LUSC" VALUE="LUSC"/>
        <ITEM NAME="PRAD" VALUE="PRAD"/>
        <ITEM NAME="READ" VALUE="READ"/>
        <ITEM NAME="SKCM" VALUE="SKCM"/>
      </ITEMS>
    </PARAMETER>
   
   
	<PARAMETER NAME="ENDPOINT" FIELD="CLOSED_LIST" NUM_ROW="12">
      <ITEMS DEFAULT="A">
        <ITEM NAME="A" VALUE="A"/>
        <ITEM NAME="B" VALUE="B"/>
        <ITEM NAME="C" VALUE="C"/>
      </ITEMS>
    </PARAMETER>
    
    
       
    <PARAMETER NAME="FORMAT_OUT" FIELD="CLOSED_LIST" NUM_ROW="15">
      <ITEMS DEFAULT="RDFXML">
        <ITEM NAME="RDFXML" VALUE="rdf"/>
        <ITEM NAME="TTL" VALUE="ttl"/>
        <ITEM NAME="NT" VALUE="nt"/>
      </ITEMS>
    </PARAMETER>
    
    <PARAMETER
      NAME="QUERY"
      FIELD="MEMO_SQL"
      NUM_ROW="55"
      REQUIRED="true"
    >
      <DEFAULT>"CONSTRUCT {?s rdf:type ?type}
WHERE
{
	?s rdf:type ?type .
}
LIMIT 10"</DEFAULT>
    </PARAMETER>

  </PARAMETERS>
  
  <ADVANCED_PARAMETERS>
	   
	    <PARAMETER NAME="CUSTOM_URL" FIELD="CHECK" NUM_ROW="5">
	      <DEFAULT>false</DEFAULT>
	   </PARAMETER>
	    
	    <PARAMETER NAME="ENDPOINT_CUSTOM_URL" FIELD="TEXT" SHOW_IF="CUSTOM_URL == 'true'" NUM_ROW="5">
	      <DEFAULT>"http://digital.path.uab.edu/sparql"</DEFAULT>
	    </PARAMETER>
	    
	    	    <PARAMETER
      NAME="URL_PARAMETERS"
      FIELD="TABLE"
      NUM_ROW="8"
      NB_LINES="4"
    >
      <ITEMS>
        <ITEM NAME="KEY" FIELD="String" VALUE="&quot;key&quot;" />
        <ITEM NAME="VALUE" FIELD="String" VALUE="&quot;value&quot;" />
      </ITEMS>
    </PARAMETER>
   
	   <PARAMETER NAME="CHECK_DISPLAY_URL" FIELD="CHECK" NUM_ROW="10">
	      <DEFAULT>false</DEFAULT>
	   </PARAMETER>

   </ADVANCED_PARAMETERS>

  <CODEGENERATION/>

  <RETURNS/>

</COMPONENT>