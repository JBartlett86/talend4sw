<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.101"
    STATUS="ALPHA"
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    DATA_AUTO_PROPAGATE="false"
    SCHEMA_AUTO_PROPAGATE="false"
    STARTABLE="true"
	PARTITIONING="AUTO"
    >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Semantic Web</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER NAME="PROPERTY" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="1">
  </PARAMETER>
    
    <PARAMETER
      NAME="QUERY"
      FIELD="MEMO_SQL"
      NUM_ROW="55"
      REQUIRED="true"
    >
      <DEFAULT>"PREFIX psys: &lt;http://proton.semanticweb.org/2006/05/protons#&gt;
PREFIX rdf: &lt;http://www.w3.org/1999/02/22-rdf-syntax-ns#&gt;
PREFIX gene: &lt;http://linkedlifedata.com/resource/entrezgene/&gt;

SELECT ?gene ?go
WHERE {
    ?gene rdf:type gene:Gene;
    gene:geneSymbol 'TP53' .
    ?gene gene:goTerm ?go .
}
LIMIT 15"</DEFAULT>
    </PARAMETER>

  </PARAMETERS>
  
  <ADVANCED_PARAMETERS>
  
	    <PARAMETER NAME="SPARQL_ENDPOINT" FIELD="TEXT" NUM_ROW="5">
	      <DEFAULT>"http://linkedlifedata.com/sparql"</DEFAULT>
	    </PARAMETER>
	    
	    	    <PARAMETER
      NAME="URL_PARAMETERS"
      FIELD="TABLE"
      NUM_ROW="8"
      NB_LINES="4"
    >
      <ITEMS>
        <ITEM NAME="KEY" FIELD="String" VALUE="&quot;key&quot;" />
        <ITEM NAME="VALUE" FIELD="String" VALUE="&quot;value&quot;" />
      </ITEMS>
    </PARAMETER>
   
	   <PARAMETER NAME="CHECK_DISPLAY_URL" FIELD="CHECK" NUM_ROW="10">
	      <DEFAULT>false</DEFAULT>
	   </PARAMETER>

   </ADVANCED_PARAMETERS>

  <CODEGENERATION>
		<IMPORTS>
			<IMPORT NAME="json-20140107.jar" MODULE="json-20140107.jar" REQUIRED="true" />
		</IMPORTS>
	</CODEGENERATION>

  <RETURNS/>

</COMPONENT>